<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <!-- language - 페이지에서 사용할 프로그래밍 언어 / 아직까지도 java만 쓸 수 있음 / 나중에 먼 훗날 JSP의 확장성을 위해서 만들어둔 속성 -->
    <!-- contentType - 이 페이지의 타입(MimeType)과 문자코드  -->
    <!-- pageEncoding - JSP 파일 내용을 저장할 때 사용할 문자코드 -->
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

</body>
</html>